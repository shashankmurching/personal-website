{
  "name": "my-site",
  "homepage": "https://shashankmurching.github.io/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-md": "^1.3.5",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.2"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js",
    "build": "react-scripts build --include-path ./node_modules src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "deploy": "npm run build&&gh-pages -b master -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "npm-run-all": "^4.1.3"
  }
}
